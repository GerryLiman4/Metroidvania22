; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[addons]

metroidvania_system/settings_file="res://MetSysSettings.tres"

[application]

config/name="Metroidvania22"
run/main_scene="res://UI/main.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
config/icon="res://icon.png"

[autoload]

SignalManager="*res://Script/Singleton/SignalManager.gd"
DialogueState="*res://Dialogue/Helpers/state.gd"
AudioController="*res://Script/Singleton/audio_controller.tscn"
SceneTransition="*res://Script/Singleton/scene_transition.tscn"
DialogueManager="*res://addons/dialogue_manager/dialogue_manager.gd"
MetSys="*res://addons/MetroidvaniaSystem/Nodes/Singleton.tscn"

[dialogue_manager]

general/states=["DialogueState"]
general/balloon_path="res://Dialogue/Dialogue/balloon.tscn"

[display]

window/size/viewport_width=1280
window/size/viewport_height=960
window/energy_saving/keep_screen_on=false
window/stretch/mode="canvas_items"

[dotnet]

project/assembly_name="Metroidvania22"

[editor_plugins]

enabled=PackedStringArray("res://addons/MetroidvaniaSystem/plugin.cfg", "res://addons/dialogue_manager/plugin.cfg", "res://addons/godot_state_charts/plugin.cfg")

[file_customization]

folder_colors={
"res://Component/": "red",
"res://Dialogue/": "yellow",
"res://Resources/": "green",
"res://Script/": "purple",
"res://UI/": "orange",
"res://addons/": "yellow",
"res://godot_state_charts_examples/": "yellow"
}

